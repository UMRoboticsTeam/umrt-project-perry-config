<?xml version='1.0' encoding='UTF-8'?>
<package format="2">
  <name>project_perry_project_perry_ikfast_plugin</name>
  <version>@version_input@</version>
  <description>IKFast plugin for project_perry</description>
  <license>BSD</license>
  <maintainer email="noah@todo.todo">noah</maintainer>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <export>
    <build_type>ament_cmake</build_type>
    <moveit_core plugin="${prefix}/project_perry_project_perry_moveit_ikfast_plugin_description.xml"/>
  </export>
  <build_depend>moveit_core</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>tf2_eigen</build_depend>
  <build_depend>tf2_kdl</build_depend>
  <build_depend>liblapack-dev</build_depend>
  <build_depend>rclcpp</build_depend>
  <exec_depend>moveit_core</exec_depend>
  <exec_depend>pluginlib</exec_depend>
  <exec_depend>liblapack-dev</exec_depend>
  <exec_depend>rclcpp</exec_depend>
</package>
